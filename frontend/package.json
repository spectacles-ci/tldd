{
    "name": "app",
    "version": "0.0.1",
    "description": "Bootstrapped Looker Extension with React and Typescript",
    "main": "dist/bundle.js",
    "author": "Josh Temple <josh@spectacles.dev>",
    "license": "Apache-2.0",
    "scripts": {
        "analyze": "export ANALYZE_MODE=static && yarn build",
        "build": "export BABEL_ENV=build && webpack --config webpack.prod.js",
        "clean": "rm -rf dist",
        "dev": "webpack serve --hot --port 8080 --https --disable-host-check --config webpack.develop.js",
        "prebuild": "yarn clean"
    },
    "dependencies": {
        "@looker/components": "^4.0.4",
        "@looker/embed-sdk": "^1.8.0",
        "@looker/extension-sdk": "^22.20.0",
        "@looker/extension-sdk-react": "^22.20.0",
        "@looker/icons": "^1.5.21",
        "@looker/sdk": "^22.20.0",
        "@looker/sdk-rtl": "^21.4.0",
        "@styled-icons/material": "10.34.0",
        "@styled-icons/material-outlined": "10.34.0",
        "@styled-icons/material-rounded": "10.34.0",
        "@tanstack/react-query": "^4.0.0",
        "clsx": "^2.1.1",
        "date-fns": "^2.25.0",
        "lodash": "^4.17.21",
        "react": "^16.14.0",
        "react-dom": "^16.14.0",
        "react-hook-form": "^7.52.0",
        "react-is": "^16.13.1",
        "react-router-dom": "^5.3.0",
        "semver": "^7.3.5",
        "styled-components": "^5.3.3"
    },
    "devDependencies": {
        "@babel/cli": "^7.16.0",
        "@babel/core": "^7.16.0",
        "@babel/plugin-proposal-class-properties": "^7.16.0",
        "@babel/plugin-proposal-object-rest-spread": "^7.16.0",
        "@babel/plugin-transform-react-jsx": "^7.16.0",
        "@babel/plugin-transform-runtime": "^7.16.0",
        "@babel/preset-env": "^7.16.0",
        "@babel/preset-react": "^7.16.0",
        "@babel/preset-typescript": "^7.16.0",
        "@babel/runtime": "^7.16.0",
        "@tailwindcss/forms": "^0.5.7",
        "@trivago/prettier-plugin-sort-imports": "^4.3.0",
        "@types/lodash": "^4.14.176",
        "@types/node": "^14.14.12",
        "@types/react": "^16.14.2",
        "@types/react-dom": "^16.9.10",
        "@types/react-router-dom": "^5.1.5",
        "@types/readable-stream": "^2.3.5",
        "@types/semver": "^7.3.1",
        "@types/styled-components": "5.1.5",
        "autoprefixer": "^10.4.19",
        "babel-loader": "^8.2.3",
        "babel-loader-exclude-node-modules-except": "^1.2.1",
        "babel-preset-nano-react-app": "^0.1.0",
        "css-loader": "^7.1.2",
        "minimist": "^1.2.5",
        "nodemon": "^2.0.14",
        "npm-run-all": "^4.1.5",
        "postcss": "^8.4.38",
        "postcss-loader": "^8.1.1",
        "postcss-preset-env": "^9.5.14",
        "prettier": "3.3.2",
        "react-hot-loader": "^4.13.0",
        "style-loader": "^4.0.0",
        "tailwindcss": "^3.4.4",
        "typescript": "4.4.4",
        "webpack": "^5.92.1",
        "webpack-bundle-analyzer": "^4.5.0",
        "webpack-cli": "^4.10.0",
        "webpack-dev-server": "^3.11.3"
    },
    "babel": {
        "presets": [
            "nano-react-app"
        ],
        "plugins": [
            [
                "@babel/plugin-proposal-class-properties"
            ],
            [
                "@babel/plugin-transform-react-jsx",
                {
                    "pragmaFrag": "React.Fragment"
                }
            ]
        ]
    }
}
